{
  "name": "@sabia/web",
  "scripts": {
    "dev": "cross-env NODE_ICU_DATA=node_modules/full-icu APP_ENV=development next dev -p 8000",
    "build": "cross-env NODE_ICU_DATA=node_modules/full-icu next build",
    "start:ci": "cross-env APP_ENV=cypress NODE_ENV=production npm run build && APP_ENV=testing npm run start",
    "start": "cross-env NODE_ICU_DATA=node_modules/full-icu NODE_ENV=production next start -p 8000",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook",
    "jest": "cross-env APP_ENV=testing NODE_ICU_DATA=node_modules/full-icu jest --coverage --maxWorkers=50%",
    "jest:ci": "cross-env APP_ENV=testing NODE_ICU_DATA=node_modules/full-icu jest --coverage --runInBand",
    "jest:watch": "cross-env APP_ENV=testing NODE_ICU_DATA=node_modules/full-icu jest --watch --maxWorkers=25%",
    "jest:visual": "cross-env APP_ENV=testing NODE_ICU_DATA=node_modules/full-icu jest -c tests/visual/jest.config.js",
    "jest:visual:update": "cross-env APP_ENV=testing npm run jest:visual -- --updateSnapshot",
    "test:visual": "cross-env APP_ENV=testing start-server-and-test storybook http-get://localhost:9009 jest:visual",
    "test": "cross-env APP_ENV=testing npm run jest && npm run test:visual"
  },
