    "next": "^9.3.2",
    "next-cookies": "^2.0.3",
    "next-i18next": "^4.4.1",
    "npm": "^6.14.4",
    "prop-types": "^15.7.2",
    "query-string": "^6.12.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-hook-form": "^5.6.3",
    "react-i18next": "^11.4.0",
    "react-icons": "^3.9.0",
    "react-instantsearch-dom": "^6.4.0",
    "react-keyed-flatten-children": "^1.2.0",
